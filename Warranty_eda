bike_counts = (
    df.groupby("BIKE_NAME")
      .size()
      .reset_index(name="total_warranty_count")
      .sort_values("total_warranty_count", ascending=False)
)

bike_counts.to_csv("bike_name_warranty_counts.csv", index=False)
